append head
  link(rel='stylesheet' href='/css/Chat/bootstrap.css')
  link(rel='stylesheet' href='/css/Chat/style.css')

block content
  .img2
    .container
      ul.list-group
        each user,i in users
          if user.username != req_user
            li.list-group-item
              a(href="/chat/"+user._id)= user.name
